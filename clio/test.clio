fn say what = 'hello' -- default values
       who  = 'Pouya':
    
    who 'says' what -> print

{ #what 'yo!' } -> say ~~@ -- Pouya says yo!
                           -- unpack dictionary

fn double ~numbers:
    numbers ->* times 2

1 2 3 4 5 -> double ~[6 7 8] -- unpack [6 7 8]

fn kwprint ~~kwargs: -- accept extra keyword args
    kwargs
        ->  unpack
        ->* print 'Keyword:' @[0]
                  'Value:'   @[1]

data -> func a b
             c d  -- c and d after -> so they're
                  -- parts of func call

data -> func a b
c d  -- c and d behind -> so not a part of func call

