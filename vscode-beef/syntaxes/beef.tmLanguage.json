{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "beef",
  "patterns": [
    {
      "begin": "{",
      "end": "}",
      "patterns": [
        {
          "include": "source.js"
        }
      ]
    },
    {
      "match": "(//.*)",
      "name": "comment.beef"
    },
    {
      "include": "#keywords"
    }
  ],
  "repository": {
    "keywords": {
      "patterns": [
        {
          "name": "keyword.other.beef",
          "match": "(=>|[|])"
        }
      ]
    }
  },
  "scopeName": "source.beef"
}